# Environment variables for RunPod serverless endpoint
# Copy this file to .env and fill in your actual values

# RunPod Configuration
RUNPOD_API_KEY=your_runpod_api_key_here

# Cloudflare R2 Configuration (matching web app variable names)
CLOUDFLARE_R2_ACCESS_KEY_ID=your_r2_access_key_id_here
CLOUDFLARE_R2_SECRET_ACCESS_KEY=your_r2_secret_access_key_here
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here
R2_BUCKET_NAME=your_r2_bucket_name_here
R2_BUCKET_PUBLIC_URL=https://pub-your-account-id.r2.dev

# Training Configuration (optional overrides)
DEFAULT_STEPS=1000
DEFAULT_LEARNING_RATE=1e-4
DEFAULT_BATCH_SIZE=1
DEFAULT_RESOLUTION=512

# Logging Configuration
LOG_LEVEL=INFO
ENABLE_WANDB=false
WANDB_PROJECT=fluxgym-runpod
